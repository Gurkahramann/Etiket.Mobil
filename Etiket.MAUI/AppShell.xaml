<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Etiket.MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Etiket.MAUI.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="Etiket.MAUI">

    <ShellContent Title="Etiket Döküm" ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage"/>
    <ShellContent Title="Yazıcı Konfigürasyonu" ContentTemplate="{DataTemplate local:PrinterConfig}" Route="PrinterConfig"/>
    
</Shell>
